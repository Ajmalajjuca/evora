{
  "expo": {
    "name": "EventHub",
    "slug": "eventhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eventhub",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ajmalca.eventhub"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E8DCC8",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "package": "com.ajmalca.eventhub"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "backgroundColor": "#E8DCC8",
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e22d6be2-37a1-4195-9bde-fac13db76cb2"
      }
    }
  }
}
